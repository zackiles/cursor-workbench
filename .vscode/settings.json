{
  "files.eol": "\n",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "editor.detectIndentation": false,
  "editor.indentSize": 2,
  "editor.insertSpaces": true,
  "json.schemaDownload.enable": true,
  "files.associations": {
    "repomix.config.json": "json5"
  },
  "[json][jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[markdown][typescript][javascript]": {
    "editor.defaultFormatter": "biomejs.biome",
    "editor.formatOnSave": true
  },
  "search.exclude": {
    "**/.cursor": true
  },
  "cSpell.dictionaries": [
    "!typescript"
  ],
  "cSpell.words": [
    ".cursorignore",
    "asynciterable",
    "cursorignoreindex",
    "dbus",
    "dconf",
    "deno",
    "denoland",
    "dockerenv",
    "dotenv",
    "frontmatter",
    "fseventsd",
    "hiberfil",
    "Iles",
    "keyrings",
    "MILLIS",
    "modelcontextprotocol",
    "msvc",
    "onclose",
    "parentbased",
    "pids",
    "repomix",
    "rwxr",
    "somevalue",
    "swapfile",
    "traceparent",
    "tracestate",
    "typeof",
    "unvalidated",
    "USERPROFILE",
    "webgpu",
    "zacharyiles",
    "zackiles",
    "Zipkin"
  ]
}
